{
	"name": "@zikyfranky/sol-sdk",
	"version": "0.0.1",
	"description": "Client SDK.",
	"repository": "https://github.com/zikyfranky/sol-sdk",
	"license": "UNLICENSED",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"@zikyfranky:registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"build-program": "anchor build --skip-lint -- --features \"feature-dev\"",
		"build-sdk": "tsc -P tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"deploy-program": "./scripts/deploy-program.sh",
		"eslint": "eslint --cache 'src/**/*.ts'",
		"lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
		"lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
		"setup": "./scripts/setup.sh",
		"test": "./scripts/test.sh",
		"test-debug": "DEBUG=true BLUEBIRD_LONG_STACK_TRACES=1 ./scripts/test.sh",
		"test-program": "jest"
	},
	"dependencies": {
		"@coral-xyz/anchor": "^0.29.0",
		"@solana/spl-token": "^0.3.9",
		"tiny-invariant": "^1.3.1"
	},
	"devDependencies": {
		"@jest/types": "^29.2.1",
		"@solana/web3.js": "^1.52.0",
		"@types/bn.js": "^5.1.0",
		"@types/jest": "^27.4.1",
		"@typescript-eslint/eslint-plugin": "^5.40.1",
		"@typescript-eslint/parser": "^5.40.1",
		"bluebird": "^3.7.2",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-sort-keys-fix": "^1.1.2",
		"eslint-plugin-typescript-sort-keys": "^2.1.0",
		"jest": "^29.7.0",
		"npm": "^8.19.2",
		"prettier": "^2.6.2",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.7.0",
		"typescript": "^4.3.5"
	}
}